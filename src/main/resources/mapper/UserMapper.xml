<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.iu.admin.mapper.UserMapper">

    <insert id="insertGroupData">
        insert into `group_name` value
            (replace(uuid(),'-',''),#{name},#{userid});
    </insert>

    <select id="selectListByUserId" resultType="cn.iu.admin.entity.GroupName">
        select * from group_name where userid = #{userId};
    </select>


</mapper>